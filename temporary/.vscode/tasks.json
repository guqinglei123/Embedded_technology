{
    "version": "0.1.0",
    "command": "g++",
    "args": ["-g","${fileDirname}/sort.cpp","-std=c++11","-o","${fileDirname}/a.exe"],
    "problemMatcher": {
        "owner": "cpp",
        "fileLocation": ["relative", "${workspaceRoot}"],
        "pattern": {
            "regexp": "^(.*):(/d+):(/d+):/s+(warning|error):/s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
        }
    }
}